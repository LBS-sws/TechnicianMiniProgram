.review_popup {
  box-sizing: border-box;
  padding: 0 24rpx 24rpx;
}
.review_popup .review_popup_title {
  text-align: center;
  padding: 24rpx;
  font-size: 45rpx;
}
.review_popup .review_popup_box {
  padding-inline: 40rpx;
  margin-bottom: 24rpx;
}
.review_popup .review_popup_box .review_popup_box_entitle {
  font-size: 35rpx;
  color: #201f1f;
  text-indent: 1rem;
}
.review_popup .review_popup_box .review_popup_box_radio {
  margin-top: 20rpx;
  display: inline-block;
  width: 100%;
}
.review_popup .review_popup_box .review_popup_box_radio > view {
  width: 45%;
  float: left;
}
.review_popup .review_popup_button {
  padding-top: 24rpx;
}
.review_popup .review_popup_button .cl-button {
  width: 100%;
  height: 70rpx;
}
