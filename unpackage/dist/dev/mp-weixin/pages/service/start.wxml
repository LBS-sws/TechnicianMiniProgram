<block wx:if="{{showContent}}"><view class="content"><view class="my_card"><view class="cust_name"><text selectable="true">{{service.customer.name_zh}}</text></view><view class="v_magin">服务次数：<label style="color:black;" class="_span">{{service.count+"次"}}</label></view><view class="v_magin">服务人员：<label style="color:black;" class="_span"><text selectable="true">{{service.customer.name_zh}}</text></label></view><view class="v_magin">联系人员：<label style="color:black;" class="_span"><text selectable="true">{{service.contact_name}}</text></label></view><view class="v_magin">联系电话：<label style="color:black;" class="_span"><text selectable="true">{{service.tel}}</text></label></view><view class="v_magin">联系地址：<label style="color:black;" class="_span"><text selectable="true">{{service.customer.addr}}</text></label></view><cl-divider vue-id="0d615007-1" bind:__l="__l" vue-slots="{{['default']}}"><text class="cl-icon-favor"></text><text style="color:#007AFF;font-size:14px;">服务流程</text></cl-divider><cl-timeline vue-id="0d615007-2" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><cl-timeline-item vue-id="{{('0d615007-3-'+index)+','+('0d615007-2')}}" timestamp="{{item.timestamp}}" show-line="{{item.showLine}}" content="{{item.content}}" icon="{{item.icon}}" color="{{item.color}}" classType="{{item.classType}}" data-event-opts="{{[['^tap',[['lc',['$0','$1'],[[['list','',index,'url']],[['list','',index,'shortcut_type']]]]]]]}}" bind:tap="__e" bind:__l="__l"></cl-timeline-item></block></cl-timeline></view><block wx:if="{{service.main_staff}}"><view class="tj_bu_y"><cl-row vue-id="0d615007-4" gutter="20" bind:__l="__l" vue-slots="{{['default']}}"><cl-col vue-id="{{('0d615007-5')+','+('0d615007-4')}}" span="12" data-event-opts="{{[['^tap',[['report']]]]}}" bind:tap="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="jc">检查报告</view></cl-col><block wx:if="{{service.status!=3&&service.finish_time!='00:00:00'}}"><cl-col vue-id="{{('0d615007-6')+','+('0d615007-4')}}" span="12" data-event-opts="{{[['^tap',[['check_out']]]]}}" bind:tap="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="qc">签出离店</view></cl-col></block><block wx:else><cl-col vue-id="{{('0d615007-7')+','+('0d615007-4')}}" span="12" bind:__l="__l" vue-slots="{{['default']}}"><view class="qc">已签离店</view></cl-col></block></cl-row></view></block><block wx:else><view data-event-opts="{{[['tap',[['report']]]]}}" class="tj_bu" bindtap="__e">检查报告</view></block></view></block>